PREREQ=""

prereqs()
{
     echo "$PREREQ"
}

case $1 in
prereqs)
     prereqs
     exit 0
     ;;
esac

. /usr/share/initramfs-tools/hook-functions

if [ -f /etc/os-release ]; then
    . /etc/os-release
fi
hook_error() {
    echo "(ERROR): $1" >&2
    exit 1
}
copy_exec /usr/bin/tpm2_pcrlist || hook_error "Unable to copy /usr/bin/tpm2_pcrlist"
